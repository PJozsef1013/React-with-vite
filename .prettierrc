{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 140,
  "bracketSpacing": true,
  "arrowParens": "always",
  "importOrder": ["^[@/(.*)|a-z|A-Z|0-9]$", "^@environment", "^@shared", "^[../]", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["prettier-plugin-tailwindcss"]
}