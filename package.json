{
  "name": "react-book-collection-tracker-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx",
    "lint:prettier": "prettier --list-different index.html \"*{.ts,.cjs}\" \"src/**/*{.ts,.tsx,.json,.css,.scss,.html}\"",
